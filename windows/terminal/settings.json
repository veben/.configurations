{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
        {
            "command": {
                "action": "closeTab"
            },
            "keys": "ctrl+w"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "down",
                "splitMode": "duplicate"
            },
            "keys": "ctrl+down"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "command": {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "right",
                "splitMode": "duplicate"
            },
            "keys": "ctrl+right"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{17bf3de4-5353-5709-bcf9-835bd952a95e}",
    "newTabMenu": [
        {
            "type": "remainingProfiles"
        }
    ],
    "profiles": {
        "defaults": {
            "adjustIndistinguishableColors": "always",
            "backgroundImage": "ms-appdata:///roaming/bg.jpg",
            "backgroundImageOpacity": 0.0,
            "backgroundImageStretchMode": "none",
            "colorScheme": "Catppuccin Mocha",
            "cursorShape": "vintage",
            "experimental.retroTerminalEffect": false,
            "font": {
                "face": "UbuntuMono Nerd Font",
                "size": 11.0,
                "weight": "normal"
            },
            "opacity": 100,
            "startingDirectory": "%USERPROFILE%",
            "useAcrylic": false
        },
        "list": [
